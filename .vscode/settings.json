{
  // - - - StatusBar settings - - - - - - - - - - - - -
  "workbench.colorCustomizations": {
    "statusBar.background": "#333333",
  },
  // - - - Folder's icon settings - - - - - - - - - - -
  "material-icon-theme.folders.associations": {
    "nginx": "Routes",
    "s3_conf": "Aws",
    "websocket": "Wordpress",
    "frontend_build": "Base",
    "frontend_dev": "Redux-store",
    "store": "Redux-store",
    "venv": "Python",
  },
  // - - - PythonPath settings- - - - - - - - - - - - - 
  "python.defaultInterpreterPath": "./venv/bin/python3",
  // - - - Formatter settings - - - - - - - - - - - - -
  "python.formatting.provider": "black",
  // - - - liveSassCompile settings - - - - - - - - - - 
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/frontend/frontend_dev/src/styles/css"
    },
  ],
  // - - - cSpell settings - - - - - - - - - - - - - - - 
  "cSpell.language": "en,ru",
  "cSpell.words": [
    "aiohttp",
    "aioredis",
    "dals",
    "DJOSER",
    "doszhan",
    "fastapi",
    "imagekit",
    "imgix",
    "mediastatic",
    "simplejwt",
    "templating",
    "testpass",
    "toastify",
    "Twotone",
    "videohub",
    "yasg"
  ],
}